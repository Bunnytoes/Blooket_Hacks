        ca.setStyle("\t-fx-background-color: #1a1915");
        Scene cA = new Scene(ca,350,350);
        cafe.setOnMouseClicked(e->{
            stage.setScene(cA);
        });


        stage.setTitle("Blooket Hacks");
        stage.setScene(mainScene);
        stage.show();
    }
static void Pause5 (Button b)
    {
        String txt = b.getText();
        b.setText("Copied");
        PauseTransition shortPause = new PauseTransition(Duration.seconds(5));
        shortPause.setOnFinished(event -> b.setText(txt));
        shortPause.playFromStart();
    }
    static void setButton(Button b, double x, double y)
    {
        Font font = Font.font("Arial", FontWeight.EXTRA_BOLD, FontPosture.REGULAR, 12.5);
        b.setStyle("\t-fx-background-color: #00002e");
        b.setFont(font);
        b.setTextFill(Color.BLACK);
        b.setTranslateY(y);
        b.setTranslateX(x);
    }
    public static void main(String[] args) {
        launch();
    }
}
